<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.gymcarry.dao.mapper.BoardMapper">

	<!-- List All -->
	<select id="listAll" resultType="com.project.gymcarry.board.BoardDto">
		<![CDATA[  // SQL 내부에 연산자가 있을 경우 사용
	select *
	from board
	where postidx > 0
	order by postidx desc, postdate desc
		]]>
	</select>


</mapper>